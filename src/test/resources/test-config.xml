<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">
      <bean class="net.sf.sze.config.RootConfig"/>
      <bean id="resourceLoader" class=
        "de.conceptpeople.checkerberry.db.spring.jpa.bridge.resource.SpringClasspathResourceLoader"></bean>
    <bean id="databaseConnector" class=
        "de.conceptpeople.checkerberry.db.spring.jpa.bridge.database.SpringJPADatabaseConnector">
        <constructor-arg ref="entityManagerFactory"></constructor-arg>
        <constructor-arg ref="dataSource"></constructor-arg>
    </bean>

</beans>
